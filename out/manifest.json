{
	"manifest_version": 2,
	
	"name": "RerunFilter",
	"description": "This extension enables you to filter rebroadcasts out of your Twitch following list.",
	"version": "1.0",
	"author": "jannik@feuer.io",
	
	"content_scripts": [
		{
			"matches": ["https://www.twitch.tv/*"],
			"js": ["jquery.js", "filter.js"]
		}
	],
	"background":
	{
		"scripts":["background.js"],
		"persistent": false
	},
	"page_action": {
		"default_icon": {
			"16": "images/get_started16.png",
			"32": "images/get_started32.png",
			"48": "images/get_started48.png",
			"128": "images/get_started128.png"
		}
	},
	"icons": {
		"16": "images/get_started16.png",
		"32": "images/get_started32.png",
		"48": "images/get_started48.png",
		"128": "images/get_started128.png"
	}
}